        "NetworkAclId": {
          "Ref": "acl05b7fe6929baa754c"
        },
        "SubnetId": {
          "Ref": "SubnetPrivateAId"
        }
      }
    },
    "subnetacl12": {
      "Type": "AWS::EC2::SubnetNetworkAclAssociation",
      "Properties": {
        "NetworkAclId": {
          "Ref": "acl05b7fe6929baa754c"
        },
        "SubnetId": {
          "Ref": "SubnetPublicAId"
        }
      }
    }
  }
}