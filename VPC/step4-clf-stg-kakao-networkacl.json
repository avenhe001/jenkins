{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Parameters": {
        "SubnetPrivateCId": {
            "Type": "AWS::EC2::Subnet::Id",
            "Description": "SubNetPrivateC"
        },
        "SubnetPrivateAId": {
            "Type": "AWS::EC2::Subnet::Id",
            "Description": "SubNetPrivateA."
        },
        "SubnetPublicAId": {
            "Type": "AWS::EC2::Subnet::Id",
            "Description": "SubnetPublicAId."
        },
        "SubnetPublicCId": {
            "Type": "AWS::EC2::Subnet::Id",
            "Description": "SubnetPublicCId."
        },
        "VpcId": {
	    "Type": "AWS::EC2::VPC::Id",
	    "Description": "Select a VPC that allows instances to access the Internet. Select same VPC in which LoadBalancer was created."
	}
  },

  "Resources": {
    "acl05b7fe6929baa754c": {
      "Type": "AWS::EC2::NetworkAcl",
      "Properties": {
        "VpcId": {
          "Ref": "VpcId"
        }
      }
    },
    "acl7": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "CidrBlock": "0.0.0.0/0",
        "Egress": "true",
        "Protocol": "-1",
        "RuleAction": "allow",
        "RuleNumber": "100",
        "NetworkAclId": {
          "Ref": "acl05b7fe6929baa754c"
        }
      }
    },
    "acl8": {
      "Type": "AWS::EC2::NetworkAclEntry",
      "Properties": {
        "CidrBlock": "0.0.0.0/0",
        "Protocol": "-1",
        "RuleAction": "allow",
        "RuleNumber": "100",
        "NetworkAclId": {
          "Ref": "acl05b7fe6929baa754c"
        }
      }
    },
    "subnetacl9": {
      "Type": "AWS::EC2::SubnetNetworkAclAssociation",
      "Properties": {
        "NetworkAclId": {
          "Ref": "acl05b7fe6929baa754c"
        },
        "SubnetId": {
          "Ref": "SubnetPrivateCId"
        }
      }
    },
    "subnetacl10": {
      "Type": "AWS::EC2::SubnetNetworkAclAssociation",
      "Properties": {
        "NetworkAclId": {
          "Ref": "acl05b7fe6929baa754c"
